{
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "func\\": "src/",
            "SecurityService\\": "src/",
            "App\\Mutator\\": "custom-mutators/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "infection/infection": "^0.29.10",
        "rector/rector": "^2.0"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "require": {
        "infection/mutator": "^0.4.0"
    }
}
